{
  "expo": {
    "name": "CandidateApp",
    "slug": "candidate-app",
    "scheme": "workforceapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.workforcesa.candidateapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "workforceapp"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c445311f-75f5-44d6-934e-e333037058ba"
      }
    }
  }
}
